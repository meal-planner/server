rameter": [          {            "group": "Parameter",            "type": "String",            "optional": true,            "field": "query",            "description": "<p>Full text search query</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": true,            "field": "filter_by",            "description": "<p>Filter attribute name (<code>id</code>,<code>group</code>, <code>owner_id</code>, <code>generic</code>, <code>ready_to_eat</code>)</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": true,            "field": "filter_value",            "description": "<p>Filter value</p>"          },          {            "group": "Parameter",            "type": "Number",            "optional": true,            "field": "start",            "description": "<p>Initial offset</p>"          },          {            "group": "Parameter",            "type": "Number",            "optional": true,            "field": "size",            "defaultValue": "12",            "description": "<p>Number of items to return</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": true,            "field": "sort",            "description": "<p>Sort by attribute name</p>"          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "total",            "description": "<p>Total number of ingredients matching the query</p>"          },          {            "group": "Success 200",            "type": "Object[]",            "optional": false,            "field": "items",            "description": "<p>List of ingredients</p>"          },          {            "group": "Success 200",            "type": "String