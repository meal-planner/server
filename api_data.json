[  {    "type": "get",    "url": "/ingredients",    "title": "List",    "version": "0.1.0",    "name": "IngredientsList",    "group": "Ingredients",    "description": "<p>Ingredients list can be used to retrieve a list of all ingredients, filtered ingredients or to perform full text search on the ingredients.</p> <p>For example, to retrieve list of all vegetables, use <code>filter_by: group</code> and <code>filter_value: Vegetables</code></p>",    "examples": [      {        "title": "Example usage:",        "content": "curl -i \"https://api.meal-planner.org/ingredients?query=tomatoes&filter_by=group&filter_value=Vegetables\"",        "type": "curl"      }    ],    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "query",            "description": "<p>Full text search query</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "filter_by",            "description": "<p>Filter attribute name (<code>group</code>, <code>owner_id</code>, <code>generic</code>, <code>ready_to_eat</code>)</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "filter_value",            "description": "<p>Filter value</p>"          },          {            "group": "Parameter",            "type": "Number",            "optional": false,            "field": "start",            "description": "<p>Initial offset</p>"          },          {            "group": "Parameter",            "type": "Number",            "optional": false,            "field": "size",            "description": "<p>Number of items to return, default is <code>12</code></p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "sort",            "description": "<p>Sort by attribute name</p>"          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "total",            "description": "<p>Total number of ingredients matching the query</p>"          },          {            "group": "Success 200",            "type": "Object[]",            "optional": false,            "field": "items",            "description": "<p>List of ingredients</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "items.id",            "description": "<p>Ingredient unique ID</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "items.name",            "description": "<p>Ingredient name</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "items.group",            "description": "<p>Ingredient group (Fruits, Vegetables, Grains, etc)</p>"          },          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "items.ndbno",            "description": "<p>Ingredient USDA database number</p>"          },          {            "group": "Success 200",            "type": "Boolean",            "optional": false,            "field": "items.generic",            "description": "<p>Is ingredient generic?</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "items.owner_id",            "description": "<p>Ingredient owner unique ID</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "items.image_url",            "description": "<p>Ingredient image URL (relative to content origin)</p>"          },          {            "group": "Success 200",            "type": "Date",            "optional": false,            "field": "items.created_at",            "description": "<p>Date when ingredient was created</p>"          },          {            "group": "Success 200",            "type": "Date",            "optional": false,            "field": "items.updated_at",            "description": "<p>Date when ingredient was updated last time</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "items.forked_from",            "description": "<p>Parent ingredient ID, if it was forked</p>"          },          {            "group": "Success 200",            "type": "Boolean",            "optional": false,            "field": "items.ready_to_eat",            "description": "<p>Is ingredient ready to be eaten without cooking?</p>"          },          {            "group": "Success 200",            "type": "Object[]",            "optional": false,            "field": "items.measures",            "description": "<p>List of ingredient measures</p>"          },          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "items.measures.qty",            "description": "<p>Measure quantity</p>"          },          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "items.measures.eqv",            "description": "<p>Measure equivalent in grams</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "items.measures.label",            "description": "<p>Measure label</p>"          },          {            "group": "Success 200",            "type": "Hash[]",            "optional": false,            "field": "items.measures.nutrients",            "description": "<p>Hash of nutrients values for this measure</p>"          }        ]      }    },    "filename": "app/ingredients/ingredient_api.rb",    "groupTitle": "Ingredients",    "sampleRequest": [      {        "url": "https://api.meal-planner.org/ingredients"      }    ]  }]
